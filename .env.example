# OpenClaw Dashboard API - Environment Variables
# Copy this to .env and fill in your values

# Gateway authentication token (from openclaw config)
OPENCLAW_AUTH_TOKEN=your_gateway_token_here

# Webhook hook token (for triggering agent tasks)
OPENCLAW_HOOK_TOKEN=your_hook_token_here

# Optional overrides (defaults are for macOS OpenClaw installs)
# DASHBOARD_PORT=18791
# OPENCLAW_WORKSPACE=~/.openclaw/workspace
# OPENCLAW_SESSIONS_FILE=~/.openclaw/agents/main/sessions/sessions.json
# OPENCLAW_SUBAGENT_RUNS=~/.openclaw/subagents/runs.json
